<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.cnd.makeproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/make-project/1">
            <name>Watermill</name>
            <c-extensions/>
            <cpp-extensions>cpp</cpp-extensions>
            <header-extensions>hpp</header-extensions>
            <sourceEncoding>UTF-8</sourceEncoding>
            <make-dep-projects/>
            <sourceRootList>
                <sourceRootElem>../../../docs</sourceRootElem>
                <sourceRootElem>../../../source</sourceRootElem>
                <sourceRootElem>../../../media</sourceRootElem>
            </sourceRootList>
            <confList>
                <confElem>
                    <name>Debug</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>Release</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>Debug_MinGW32</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>Debug_MinGW64</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>Debug_Cygwin64</name>
                    <type>1</type>
                </confElem>
            </confList>
            <formatting>
                <project-formatting-style>false</project-formatting-style>
            </formatting>
        </data>
    </configuration>
</project>
